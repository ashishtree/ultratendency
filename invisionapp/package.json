{
  "name": "medium-react-material-ts-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.7.1",
    "@material-ui/icons": "^3.0.1",
    "@types/react-facebook-login": "^4.1.1",
    "@types/react-redux": "^7.1.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-mock-store": "^1.0.2",
    "@types/redux-promise": "^0.5.29",
    "axios": "^0.21.1",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.1.19",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-promise": "^0.6.0",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0",
    "ts-react-google-login-component": "^1.1.8"
  },
  "scripts": {
    "build:css": "node-sass src/styles/sass/ -o src/styles/css/",
    "watch:css": "npm run build:css && node-sass src/styles/sass/ -o src/styles/css/ --watch --recursive",
    "start:js": "react-scripts-ts start",
    "start": "set PORT=3006 && npm-run-all -p watch:css start:js",
    "build": "npm run build:css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/node-sass": "^3.10.32",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.2.2"
  }
}
